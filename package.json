{
  "name": "admin-panel-scanner",
  "version": "1.0.0",
  "description": "Admin Panel for Website Vulnerability Scanner",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend": "node server.js",
    "scan": "python3 backend/scanner.py",
    "scan:advanced": "python3 backend/advanced_scanner.py",
    "scan:ultimate": "python3 backend/ultimate_scanner.py",
    "start:all": "concurrently \"npm run dev\" \"npm run backend\"",
    "start": "npm run start:all"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8"
  }
}
